<?xml version="1.0" encoding="UTF-8"?>
<connector xmlns="http://java.sun.com/xml/ns/j2ee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://java.sun.com/xml/ns/j2ee http://java.sun.com/xml/ns/j2ee/connector_1_5.xsd"
	version="1.5">

	<display-name>activiti JobExecutor EE Resource Adapter</display-name>
	<vendor-name>org.agito</vendor-name>
	<eis-type>activiti</eis-type>
	<resourceadapter-version>1.0</resourceadapter-version>

	<resourceadapter>
		<resourceadapter-class>org.agito.activiti.jobexecutor.JobExecutorResourceAdapter</resourceadapter-class>
		<outbound-resourceadapter>
			<connection-definition>
				<managedconnectionfactory-class>org.agito.activiti.jobexecutor.JobExecutorManagedConnectionFactory</managedconnectionfactory-class>
				<connectionfactory-interface>org.agito.activiti.jobexecutor.api.JobExecutorRegistryFactory</connectionfactory-interface>
				<connectionfactory-impl-class>org.agito.activiti.jobexecutor.impl.JobExecutorRegistryFactoryImpl</connectionfactory-impl-class>
				<connection-interface>org.agito.activiti.jobexecutor.api.JobExecutorRegistry</connection-interface>
				<connection-impl-class>org.agito.activiti.jobexecutor.impl.JobExecutorRegistryImpl</connection-impl-class>
			</connection-definition>
			<transaction-support>NoTransaction</transaction-support>
			<reauthentication-support>false</reauthentication-support>
		</outbound-resourceadapter>
	</resourceadapter>

</connector>